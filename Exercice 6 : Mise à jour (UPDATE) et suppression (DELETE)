UPDATE etudiants
SET email = 'labe.pierre@example.com'
WHERE id_etudiant = 1;
UPDATE cours
SET nom_cours = 'Mathématiques avancées'
WHERE id_cours = 1;
DELETE FROM inscriptions
WHERE id_etudiant = 3;
DELETE FROM etudiants
WHERE id_etudiant = 3;
DELETE FROM inscriptions
WHERE id_cours = 2;
DELETE FROM cours
WHERE id_cours = 2;

